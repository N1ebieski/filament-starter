{
    "name": "Laradock Filament Starter",
    "dockerComposeFile": "../../laradock-filament-starter/docker-compose.yml",
    "remoteUser": "laradock",
    "runServices": [
        "nginx",
        "mysql",
        "redis",
	    "php-fpm",
        "docker-in-docker",
    ],
    "service": "workspace",
    "workspaceFolder": "/var/www/filament-starter.test",
    "shutdownAction": "stopCompose",
    "postCreateCommand": "uname -a",
    "customizations": {
        "vscode": {
            "extensions": [
                "amiralizadeh9480.laravel-extra-intellisense",
                "austenc.laravel-blade-spacer",
                "bmewburn.vscode-intelephense-client",
                "calebporzio.better-phpunit",
                "${containerWorkspaceFolder}/.devcontainer/extensions/laravel-goto-view-1.3.11.vsix",
                "dbaeumer.vscode-eslint",
                "eamodio.gitlens",
                "GitHub.vscode-pull-request-github",
                "MehediDracula.php-namespace-resolver",
                "recca0120.vscode-phpunit",
                "redhat.vscode-yaml",
                "xdebug.php-debug",
                "amirmarmul.laravel-blade-vscode",
                "sanderronde.phpstan-vscode",
                "pcbowers.alpine-intellisense",
                "esbenp.prettier-vscode",
                "ms-vscode.test-adapter-converter",
                "hbenl.vscode-test-explorer",
                "bradlc.vscode-tailwindcss",
                "open-southeners.laravel-pint",
                "github.vscode-github-actions",
                "EditorConfig.EditorConfig",
                "Codeium.codeium",
                "laravel.vscode-laravel",
                "dbcode.dbcode"
            ]
        }
    }
}
